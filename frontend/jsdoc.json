{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules/|__tests__/|__mocks__/)"
  },
  "plugins": [],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  }
}
